{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": 2,
        "no-underscore-dangle": "off",
        "no-async-promise-executor": 0,
        "no-undef": 0,
        "no-unused-vars": "off",
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages",
                "mjs": "ignorePackages"
            }
        ]
    }
}